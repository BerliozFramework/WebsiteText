    "berlioz/html-selector": "@dev",
    "erusev/parsedown-extra": "~0.7",
    "gregwar/rst": "~1.0",
    "php-http/httplug": "~1.1",
    "php-http/message-factory": "~1.0",
    "psr/http-message": "~1.0",
    "psr/log": "~1.0",
    "psr/simple-cache": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.2"
  }
}
